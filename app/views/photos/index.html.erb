<% if @photos.any? %>
    <% @photos.each do |photo| %>
        <h2> <%= photo.caption %> </h2>
        <%= link_to "Edit #{photo.caption}",edit_photo_path(photo) %>
        <%= link_to "Delete #{photo.caption}",photo_path(photo), method: :delete %>
        <%= link_to "Show #{photo.caption}",photo_path(photo)%>
    <% end %>
<% else %>
    No photos yet
<% end %>

<%=link_to "Add a photo", new_photo_path %>